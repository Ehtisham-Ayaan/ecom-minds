<p id="notice"><%= notice %></p>

<h1>Savermarts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @savermarts.each do |savermart| %>
      <tr>
        <td><%= link_to 'Show', savermart %></td>
        <td><%= link_to 'Edit', edit_savermart_path(savermart) %></td>
        <td><%= link_to 'Destroy', savermart, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Savermart', new_savermart_path %>
